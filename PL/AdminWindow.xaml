<Window x:Class="PL.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Add new Admin" Click="MoveToAddNewAdmin" HorizontalAlignment="Left" Margin="351,377,0,0" VerticalAlignment="Top"/>
        <Button Content="move to products window" HorizontalAlignment="Left" Margin="175,159,0,0" VerticalAlignment="Top" Click="MoveToProductsWindow"/>
        <Button Content="move to orders window" HorizontalAlignment="Left" Margin="472,159,0,0" VerticalAlignment="Top" Click="MoveToOrdersWindow" Width="140"/>
        <Button Content="Show admins" Click="ShowAdminsWindow" HorizontalAlignment="Left" Margin="175,377,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.163,2.542"/>
        <Button Content="Remove admins" Click="MoveToRemoveAdmin" HorizontalAlignment="Left" Margin="538,377,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.163,2.542"/>

    </Grid>
</Window>
